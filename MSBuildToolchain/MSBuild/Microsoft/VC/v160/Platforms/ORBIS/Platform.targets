<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_FullIntDirPath>$([MSBuild]::Escape($([System.IO.Path]::GetFullPath(`$([System.IO.Path]::Combine(`$(MSBuildProjectDirectory)`, `$(IntDir)`))`))))</_FullIntDirPath>
    <ExtensionsToDeleteOnClean Condition="Exists('$(_FullIntDirPath)\modifybuild.tmp')">modifybuild.tmp;$(ExtensionsToDeleteOnClean)</ExtensionsToDeleteOnClean>
  </PropertyGroup>
  
  <Import Condition="Exists('$(_FullIntDirPath)\modifybuild.tmp')" Project="$(_FullIntDirPath)\modifybuild.tmp"/>
  <Import Project="$(VCTargetsPath)\Platforms\ORBIS\Microsoft.Cpp.orbis.targets"/>
</Project>