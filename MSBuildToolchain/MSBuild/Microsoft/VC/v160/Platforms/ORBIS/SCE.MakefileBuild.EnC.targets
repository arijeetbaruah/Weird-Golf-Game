<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Target Name="SceEnCBuild">
    <Exec Command="$(NMakeBuildCommandLine)" Condition="'$(NMakeBuildCommandLine)'!=''"/>
  </Target>

  <!-- Needs to be in a separate .targets file for Makefile that is conditionally imported -->
  <Target Name="Build" DependsOnTargets="PrepareForNMakeBuild">
    <CallTarget Targets="SceEnCBuild" />
  </Target>

</Project>