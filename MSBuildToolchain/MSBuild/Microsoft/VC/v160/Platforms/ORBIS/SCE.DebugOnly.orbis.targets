<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <UseORBISDefaultPropertyPageSchemas>false</UseORBISDefaultPropertyPageSchemas>
    <UseORBISDefaultProjectTools>false</UseORBISDefaultProjectTools>
  </PropertyGroup>
  
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />

  <ItemGroup>
    <PropertyPageSchema Include="$(VCTargetsPath)$(LangID)\debugger_general.xml;$(VCTargetsPath)\Platforms\ORBIS\$(SceLangID)\orbisdebugger_*.xml" />
    <PropertyPageSchema Include="$(VCTargetsPath)\Platforms\ORBIS\$(SceLangID)\orbisgeneral_do.xml">
      <Context>Project</Context>
    </PropertyPageSchema>
  </ItemGroup>

</Project>
