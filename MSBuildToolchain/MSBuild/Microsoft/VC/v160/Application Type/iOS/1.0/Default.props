<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- Import Before -->
  <Import Condition="Exists('$(MSBuildThisFileDirectory)ImportBefore\Default')" Project="$(MSBuildThisFileDirectory)ImportBefore\Default\*.props" />
  
  <PropertyGroup>
    <iOSCommonTargetsPath>$(VCTargetsPath)\Application Type\$(ApplicationType)\$(ApplicationTypeRevision)\</iOSCommonTargetsPath>
    <!-- PlatformToolset when none is provided. -->
    <PlatformToolset Condition="'$(PlatformToolset)' == ''">XCode_6_1</PlatformToolset>
    <!-- Fallback PlatformToolset to use when a given toolset is not installed -->
    <DefaultPlatformToolset>XCode_6_1</DefaultPlatformToolset>
    <UseOfStl Condition="'$(UseOfStl)' == ''">libc++</UseOfStl>
    <ConfigurationType>StaticLibrary</ConfigurationType>
    <TargetOSAndVersion>iOS</TargetOSAndVersion>
    <OrganizationName Condition="'$(OrganizationName)' == ''">$(Registry:HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows NT\CurrentVersion@RegisteredOrganization)</OrganizationName>
    <OrganizationName Condition="'$(OrganizationName)' == ''">MyOrganizationName</OrganizationName>
    <RemoteRootDirectory Condition="'$(RemoteRootDirectory)' == ''">$(Registry:HKEY_CURRENT_USER\Software\Microsoft\VisualStudio\MDD\iOS@RemoteRoot)</RemoteRootDirectory>
    <RemoteRootDirectory Condition="'$(RemoteRootDirectory)' == '' AND '$(MacUserName)' != ''">/Users/$(MacUserName)/vcremote/</RemoteRootDirectory>

    <_PlatformToolsetFriendlyNameFor_Clang_3_4 Condition="'$(_PlatformToolsetFriendlyNameFor_Clang_3_4)' == ''">XCode Default</_PlatformToolsetFriendlyNameFor_Clang_3_4>
    <_UpgradePlatformToolsetFor_Clang_3_4 Condition="'$(_UpgradePlatformToolsetFor_Clang_3_4)' == ''">XCode_6_1</_UpgradePlatformToolsetFor_Clang_3_4>
    <_PlatformToolsetFriendlyNameFor_XCode_6_1 Condition="'$(_PlatformToolsetFriendlyNameFor_XCode_6_1)' == ''">XCode 6.1</_PlatformToolsetFriendlyNameFor_XCode_6_1>
    <_UpgradePlatformToolsetFor_XCode_6_1 Condition="'$(_UpgradePlatformToolsetFor_XCode_6_1)' == ''">XCode_6_1</_UpgradePlatformToolsetFor_XCode_6_1>
  </PropertyGroup>

  <!-- Import After -->
  <Import Condition="Exists('$(MSBuildThisFileDirectory)ImportAfter\Default')" Project="$(MSBuildThisFileDirectory)ImportAfter\Default\*.props" />
</Project>
